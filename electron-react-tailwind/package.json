{
  "name": "dawn-desktop-pro",
  "version": "1.0.0",
  "description": "DAWn â€” AI-powered Digital Audio Workstation",
  "author": "Abhiprit Jagvir",
  "private": true,
  "main": "electron/main.js",
  "scripts": {
    "dev": "concurrently -k \"npm:dev:*\"",
    "dev:renderer": "vite",
    "dev:electron": "wait-on tcp:5173 && cross-env VITE_DEV_SERVER_URL=http://localhost:5173 electron .",
    "build": "npm run build:renderer && npm run build:electron",
    "typecheck": "tsc --noEmit",
    "build:renderer": "vite build",
    "build:electron": "cross-env NODE_ENV=production electron-builder --dir",
    "start": "electron ."
  },
  "dependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@types/react": "^18.2.0",
