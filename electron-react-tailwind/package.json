    "autoprefixer": "^10.4.17",
    "concurrently": "^8.2.0",
    "cross-env": "^7.0.3",
    "electron": "^28.2.0",
    "electron-builder": "^24.6.0",
    "postcss": "^8.4.35",
    "tailwindcss": "^3.4.3",
    "typescript": "^5.4.0",
    "vite": "^5.0.0",
    "wait-on": "^7.2.0",
    "@types/node": "^20.10.0",
    "esbuild": "^0.21.5",
    "@vitejs/plugin-react": "^4.3.1"
  }
  
}